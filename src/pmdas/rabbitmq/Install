# Install the rabbitmq pmda
#

. $PCP_DIR/etc/pcp.env
. $PCP_SHARE_DIR/lib/pmdaproc.sh

iam=rabbitmq
python_opt=true
daemon_opt=false

pmdaSetup
pmdaInstall
exit
